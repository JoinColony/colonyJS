{
  "name": "@colony/colony-starter-react",
  "version": "0.0.0-beta.0",
  "description": "A simple starter project for building with Colony",
  "license": "MIT",
  "engines": {
    "node": "10.12.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "initialize": "sh scripts/initialize_project.sh",
    "start-ganache": "sh scripts/start_ganache.sh",
    "deploy-contracts": "sh scripts/deploy_contracts.sh",
    "start-trufflepig": "sh scripts/start_trufflepig.sh",
    "build": "webpack -p --config webpack/production.config.js",
    "start": "webpack-dev-server --config webpack/development.config.js",
    "test": "jest"
  },
  "jest": {
    "rootDir": "client",
    "testURL": "http://localhost"
  },
  "dependencies": {
    "@colony/colony-js-adapter-ethers": "1.6.0",
    "@colony/colony-js-client": "1.6.1",
    "@colony/colony-js-contract-loader-http": "1.5.4",
    "@colony/colony-js-contract-loader-network": "1.6.2",
    "bn.js": "^4.11.8",
    "buffer": "^5.1.0",
    "ethers": "^3.0.17",
    "ipfs": "^0.29.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "ganache-cli": "^6.1.0",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "precss": "^1.4.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "trufflepig": "^1.0.4",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.3"
  }
