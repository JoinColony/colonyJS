{
  "name": "@colony/colony-starter-vue",
  "version": "1.0.0-beta.0",
  "description": "A simple starter project for building with Colony",
  "keywords": [
    "colony",
    "boilerplate",
    "vue"
  ],
  "license": "MIT",
  "files": [
    "/",
    ".gitignore"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start-ganache": "colony service start-ganache",
    "deploy-contracts": "colony service deploy-contracts",
    "start-trufflepig": "colony service start-trufflepig",
    "seed-network": "colony service seed-network",
    "colony-setup": "colony service colony-setup",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test:unit": "vue-cli-service test:unit",
    "test": "yarn lint && yarn test:e2e && yarn test:unit"
  },
  "dependencies": {
    "@colony/colony-cli": "^1.0.0-beta.5",
    "@colony/colony-js-client": "^1.13.2",
    "@colony/purser-metamask": "^2.2.0",
    "core-js": "^3.1.4",
    "ipfs": "^0.36.4",
    "vue": "^2.6.10",
    "web3": "^1.0.0-beta.55",
    "web3-utils": "^1.0.0-beta.55"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-e2e-cypress": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.9.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^6.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "vue-template-compiler": "^2.6.10"
  },
  "resolutions": {
    "web3": "^1.0.0-beta.55"
  },
  "engines": {
    "node": ">=10.13.0 <12.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
